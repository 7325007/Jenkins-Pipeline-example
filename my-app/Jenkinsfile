pipeline {
    agent any

    tools {
        maven "MAVEN"
        jdk "JDK"
    }

    stages {
        
        stage('Build') {
            steps {
                //git 'https://github.com/akashpadwal/jenkins-pipeline-example.git'
                sh 'mvn -B -DskipTests clean package'
            }

            
        }
        
    }
}